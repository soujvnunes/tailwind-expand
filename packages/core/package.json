{
  "name": "@tailwind-expand/core",
  "version": "0.1.0",
  "description": "Core utilities for tailwind-expand",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.js"
    }
  },
  "files": ["dist"],
  "scripts": {
    "build": "tsup",
    "test": "vitest",
    "test:run": "vitest run"
  },
  "keywords": ["tailwind", "css", "expand"],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/soujvnunes/tailwind-expand",
    "directory": "packages/core"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "postcss": "^8.4.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "tsup": "^8.0.0",
    "typescript": "^5.4.0",
    "vitest": "^1.4.0"
  }
}
