{
  "name": "@tailwind-expand/babel",
  "version": "0.3.0",
  "description": "Babel plugin for tailwind-expand",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "test": "vitest",
    "test:run": "vitest run"
  },
  "keywords": [
    "tailwind",
    "css",
    "babel",
    "plugin"
  ],
  "author": "Victor Nunes",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/soujvnunes/tailwind-expand.git",
    "directory": "packages/babel"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@tailwind-expand/core": "workspace:*"
  },
  "peerDependencies": {
    "@babel/core": ">=7.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.24.0",
    "@babel/preset-react": "^7.24.0",
    "@types/babel__core": "^7.20.0",
    "@types/node": "^20.0.0",
    "tsup": "^8.0.0",
    "typescript": "^5.4.0",
    "vitest": "^1.4.0"
  }
}
