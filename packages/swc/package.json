{
  "name": "@tailwind-expand/swc",
  "version": "0.1.0",
  "description": "SWC plugin for tailwind-expand - expand CSS aliases in SWC-based tools",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "tailwind_expand_swc.wasm"
  ],
  "scripts": {
    "build:wasm": "cargo build-wasi --release && cp target/wasm32-wasip1/release/tailwind_expand_swc.wasm .",
    "build:ts": "tsup",
    "build": "pnpm build:wasm && pnpm build:ts",
    "prepublishOnly": "pnpm run build"
  },
  "keywords": [
    "swc",
    "swc-plugin",
    "tailwind",
    "tailwindcss",
    "nextjs",
    "turbopack",
    "rspack"
  ],
  "author": "Vitor Nunes",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/soujvnunes/tailwind-expand.git",
    "directory": "packages/swc"
  },
  "engines": {
    "node": ">=18"
  },
  "dependencies": {
    "@tailwind-expand/core": "workspace:*"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "tsup": "^8.0.0",
    "typescript": "^5.4.0"
  }
}
