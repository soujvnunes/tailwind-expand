{
  "name": "@tailwind-expand/vite",
  "version": "0.4.0",
  "description": "Vite plugin for tailwind-expand",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "test": "vitest",
    "test:run": "vitest run"
  },
  "keywords": [
    "tailwind",
    "css",
    "vite",
    "plugin"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/soujvnunes/tailwind-expand",
    "directory": "packages/vite"
  },
  "publishConfig": {
    "access": "public"
  },
  "peerDependencies": {
    "vite": ">=5.0.0"
  },
  "dependencies": {
    "@tailwind-expand/core": "workspace:*"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "tsup": "^8.0.0",
    "typescript": "^5.4.0",
    "vite": "^7.2.4",
    "vitest": "^1.4.0"
  }
}
