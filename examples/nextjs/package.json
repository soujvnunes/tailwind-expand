{
  "name": "nextjs",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:watch": "nodemon",
    "build": "next build",
    "start": "next start",
    "lint": "eslint"
  },
  "nodemonConfig": {
    "watch": [
      "app/globals.css"
    ],
    "ext": "css",
    "ignore": [
      ".next",
      "node_modules"
    ],
    "exec": "next dev"
  },
  "dependencies": {
    "next": "16.0.7",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "tailwind-merge": "^3"
  },
  "devDependencies": {
    "@tailwind-expand/postcss": "workspace:*",
    "@tailwind-expand/swc": "workspace:*",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "babel-plugin-react-compiler": "1.0.0",
    "eslint": "^9",
    "eslint-config-next": "16.0.7",
    "nodemon": "^3",
    "tailwindcss": "^4",
    "typescript": "^5"
  }
}
